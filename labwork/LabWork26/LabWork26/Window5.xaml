<Window x:Class="LabWork26.Window5"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabWork26"
        mc:Ignorable="d"
        Title="Window4" Height="500" Width="1000">
    <DockPanel>
        <WrapPanel VerticalAlignment="Top" Orientation="Horizontal" DockPanel.Dock="Top">
            <Button x:Name="Task1Button" Content="Задание 1" Width="200" Background="GreenYellow"/>
            <Button x:Name="Task2Button" Content="Задание 2" Width="200" Background="GreenYellow"/>
            <Button x:Name="Task3Button" Content="Задание 3" Width="200" Background="GreenYellow"/>
            <Button x:Name="Task4Button" Content="Задание 4" Width="200" Background="GreenYellow"/>
            <Button x:Name="Task5Button" Content="Задание 5" Width="200" Background="GreenYellow"/>
        </WrapPanel>
        <Label Content="Подвал" Background="Moccasin" DockPanel.Dock="Bottom"/>
        <Label Content="Меню" Background="PaleVioletRed"/>
        <Canvas Background="AntiqueWhite">
            <Image Source="/dragon.png" Canvas.Top="20" Canvas.Right="30" Height="170" Width="170"/>
            <Image Source="/floor.jpg" Canvas.Top="20" Canvas.Left="30" Height="170" Width="170"/>
            <Image Source="/picturechinatown.png" Canvas.Bottom="20" Canvas.Left="30" Height="170" Width="170"/>
            <Image Source="/psycho.jpg" Canvas.Bottom="20" Canvas.Right="30" Height="170" Width="170"/>
        </Canvas>
    </DockPanel>
</Window>